{
    "name": "free-games-bot",
    "version": "2.0.0-SNAPSHOT",
    "description": "Free games bot for discord",
    "main": "index.js",
    "scripts": {
        "docker-run": "docker run --rm --name free-games-bot -it -v $(pwd)/conf:/opt/free-games-bot/conf:rw fbrossar25/free-games-bot",
        "docker-bash": "docker run --rm --name free-games-bot -it -v $(pwd)/conf:/opt/free-games-bot/conf:rw -exec fbrossar25/free-games-bot /bin/bash",
        "docker-build": "docker build --tag fbrossar25/free-games-bot .",
        "start": "TZ=Europe/Paris CONF_DIR=$(pwd)/conf node index.js",
        "dev": "nodemon --inspect index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fbrossar25/Free-Games-Bot.git"
    },
    "keywords": [
        "discord",
        "bot",
        "free",
        "games"
    ],
    "author": "Florian BROSSARD",
    "license": "Unlicense",
    "bugs": {
        "url": "https://github.com/fbrossar25/Free-Games-Bot/issues"
    },
    "homepage": "https://github.com/fbrossar25/Free-Games-Bot#readme",
    "dependencies": {
        "axios": "^0.27.2",
        "bufferutil": "^4.0.6",
        "dayjs": "^1.11.5",
        "discord.js": "^14.3.0",
        "erlpack": "github:discord/erlpack",
        "node-schedule": "^1.3.3",
        "utf-8-validate": "^5.0.9",
        "zlib-sync": "^0.1.7"
    },
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=16.17.0"
    },
    "devDependencies": {
        "eslint": "^8.23.0"
    }
}